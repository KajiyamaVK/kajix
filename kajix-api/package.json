{
  "name": "kajix-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --config ../eslint.config.mjs \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e .env.test -- npm run test:db:migrate && dotenv -e .env.test -- jest --config ./test/jest-e2e.json --detectOpenHandles",
    "test:db:migrate": "dotenv -e .env.test -- prisma migrate deploy",
    "db:reset:test": "dotenv -e .env.test -- prisma migrate reset --force",
    "db:migrate": "prisma migrate deploy",
    "db:reset": "prisma migrate reset --force",
    "db:migrate:all": "npm run db:migrate && npm run test:db:migrate",
    "db:reset:all": "npm run db:reset && npm run test:db:reset",
    "seed": "prisma db seed",
    "seed:llms": "ts-node prisma/seeds/llm.seed.ts",
    "seed:users": "ts-node prisma/seeds/user.seed.ts",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "prisma:generate": "prisma generate",
    "dev": "nest start --watch",
    "test:all": "npm run prisma:generate && npm run test && npm run test:e2e",
    "type-check": "tsc --noEmit",
    "verify": "npm run lint && npm run type-check && npm run test:all && npm run build"
  },
  "dependencies": {
    "@faker-js/faker": "^9.4.0",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^11.0.6",
    "@types/cache-manager": "^4.0.6",
    "@types/ioredis": "^4.28.10",
    "@types/nodemailer": "^6.4.17",
    "bcrypt": "^5.1.1",
    "cache-manager": "^6.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "ioredis": "^5.4.2",
    "node-html-markdown": "^1.3.0",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "playwright": "^1.51.0",
    "prisma-factory": "^0.1.13",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@prisma/client": "^5.8.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.5.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.0.0",
    "prisma": "^5.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
