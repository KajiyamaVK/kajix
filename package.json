{
  "name": "kajix",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:all",
    "test:all": "npm run type-check:types && npm run test:api",
    "test:api": "cd kajix-api && npm run test",
    "test:api:e2e": "cd kajix-api && npm run test:e2e",
    "test:db:migrate": "cd kajix-api && npm run test:db:migrate",
    "prepare": "node .husky/install.mjs",
    "lint": "eslint --config ./eslint.config.mjs \"{kajix-api/src,kajix-api/test,kajix-ui/app,kajix-ui/components,kajix-ui/hooks,kajix-ui/lib,kajix-ui/styles}/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:ui": "cd kajix-ui && npm run lint",
    "type-check": "npm run type-check:types && npm run build:types && npm run type-check:api && npm run type-check:ui",
    "type-check:api": "cd kajix-api && tsc --noEmit",
    "type-check:ui": "cd kajix-ui && tsc --noEmit",
    "type-check:types": "cd packages/types && tsc --noEmit",
    "build": "npm run build:all",
    "build:types": "cd packages/types && npm run build",
    "build:api": "cd kajix-api && npm run build",
    "build:ui": "cd kajix-ui && npm run build",
    "watch:types": "cd packages/types && npm run watch",
    "build:all": "npm run build:types && npm run build:api && npm run build:ui"
  },
  "workspaces": [
    "kajix-api",
    "kajix-ui",
    "packages/types"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@reduxjs/toolkit": "^2.5.1",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/react-redux": "^7.1.34",
    "i18next": "^24.2.2",
    "lucide-react": "^0.475.0",
    "next": "^14.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.0",
    "react-redux": "^9.2.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.1.4",
    "@eslint/js": "^8.56.0",
    "@tailwindcss/postcss": "^4.0.5",
    "@types/node": "^22.13.1",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.0.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.1",
    "prettier": "^3.2.5",
    "tailwindcss": "^4.0.5",
    "typescript-eslint": "^7.3.1"
  }
}
